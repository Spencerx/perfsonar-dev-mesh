    "tests": {
        "throughput_iperf3_tcp_test_v4": {
            "type": "throughput",
            "spec": {
                "source": "{% address[0] %}",
                "dest": "{% address[1] %}",
                "duration": "PT20S",
                "ip-version": 4
            }
        },
        "throughput_iperf3_tcp_test_v6": {
            "type": "throughput",
            "spec": {
                "source": "{% address[0] %}",
                "dest": "{% address[1] %}",
                "duration": "PT10S",
                "ip-version": 6
            }
        },
        "throughput_iperf3_udp_test_v4": {
            "type": "throughput",
            "spec": {
                "source": "{% address[0] %}",
                "dest": "{% address[1] %}",
                "duration": "PT10S",
                "ip-version": 4,
                "udp": true,
                "bandwidth": 50000000,
                "window-size": 1024000
            }
        },
        "throughput_iperf3_udp_test_v6": {
            "type": "throughput",
            "spec": {
                "source": "{% address[0] %}",
                "dest": "{% address[1] %}",
                "duration": "PT10S",
                "ip-version": 6,
                "udp": true,
                "bandwidth": 50000000,
                "window-size": 1024000
            }
        },
        "throughput_nuttcp_udp_test_v4": {
            "type": "throughput",
            "spec": {
                "source": "{% address[0] %}",
                "dest": "{% address[1] %}",
                "duration": "PT10S",
                "ip-version": 4,
                "udp": true,
                "bandwidth": 50000000,
                "interval": "PT1S"
            }
        },
        "throughput_nuttcp_udp_test_v6": {
            "type": "throughput",
            "spec": {
                "source": "{% address[0] %}",
                "dest": "{% address[1] %}",
                "duration": "PT10S",
                "ip-version": 6,
                "udp": true,
                "bandwidth": 50000000,
                "interval": "PT1S"
            }
        },
        "latencybg_test_v4": {
            "type": "latencybg",
            "spec": {
                "source": "{% address[0] %}",
                "dest": "{% address[1] %}",
                "packet-timeout": 10,
                "packet-interval": 0.1,
                "packet-count": 600,
                "packet-padding": 0,
                "bucket-width": 0.001,
                "ip-version": 4
            }
        },
        "latencybg_test_v6": {
            "type": "latencybg",
            "spec": {
                "source": "{% address[0] %}",
                "dest": "{% address[1] %}",
                "packet-timeout": 10,
                "packet-interval": 0.1,
                "packet-count": 600,
                "packet-padding": 0,
                "bucket-width": 0.001,
                "ip-version": 6
            }
        },
        "trace_test_v4": {
            "type": "trace",
            "spec": {
                "source": "{% address[0] %}",
                "dest": "{% address[1] %}",
                "wait": "PT5S",
                "sendwait": "PT30S",
                "ip-version": 4
            }
        },
        "trace_test_v6": {
            "type": "trace",
            "spec": {
                "source": "{% address[0] %}",
                "dest": "{% address[1] %}",
                "wait": "PT5S",
                "sendwait": "PT30S",
                "ip-version": 6
            }
        },
        "rtt_test_v4": {
            "type": "rtt",
            "spec": {
                "source": "{% address[0] %}",
                "dest": "{% address[1] %}",
                "count": 10,
                "interval": "PT1S",
                "length": 1000,
                "ip-version": 4
            }
        },
        "rtt_test_v6": {
            "type": "rtt",
            "spec": {
                "source": "{% address[0] %}",
                "dest": "{% address[1] %}",
                "count": 10,
                "interval": "PT1S",
                "length": 1000,
                "ip-version": 6
            }
        }
    },
    
    "schedules": {
        "repeat-PT4H": {
            "repeat": "PT4H",
            "sliprand": true,
            "slip": "PT4H"
        },
        "repeat-PT10M": {
            "repeat": "PT10M",
            "sliprand": true,
            "slip": "PT10M"
        },
        "repeat-PT15M": {
            "repeat": "PT4H",
            "sliprand": true,
            "slip": "PT15M"
        }
    },
    
    "tasks": {
        "throughput-tcp-iperf3-v4": {
